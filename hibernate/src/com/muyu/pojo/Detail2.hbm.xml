<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.muyu.pojo.Detail2" table="detail2" schema="corporationservices">
        <id name="detid" column="detid">
            <generator class="native"></generator>
        </id>
        <property name="username" column="username"/>
        <property name="birthday" column="birthday"/>
        <property name="email" column="email"/>
        <many-to-one name="account2" cascade="all" unique="true" class="com.muyu.pojo.Account2">
            <column name="accid" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>