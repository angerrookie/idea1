<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.muyu.pojo.Employee" table="employee" schema="corporationservices">
        <id name="eno" column="eno">
            <generator class="native"></generator>
        </id>
        <property name="ename" column="ename"/>
        <set name="projects" table="pno_eno" inverse="true">
            <key>
                <column name="eno" not-null="true"/>
            </key>
            <many-to-many not-found="ignore" column="pno" class="com.muyu.pojo.Project"/>
        </set>
    </class>
</hibernate-mapping>