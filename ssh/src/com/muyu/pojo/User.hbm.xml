<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.muyu.pojo.User" table="user" schema="ssh">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="pass">
            <column name="pass" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>